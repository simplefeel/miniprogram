{
    "parser": "babel-eslint",
    "plugins": [
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
    },
    "ecmaFeatures": {
    },
    "rules": {
        //代码检查
        //警告
        "no-empty-function":1,
        "no-script-url":1,
        "no-unused-vars":1,
        "no-undefined":1,
        "no-wrap-func":1,

        //错误
        "no-restricted-globals":2,
        "no-use-before-define":2,
        "no-void":2,
        "no-caller":2,
        "no-alert":2,
        "no-eval":2,
        "no-debugger": 2,
        "no-console":2,
        "no-delete-var": 2,
        "no-constant-condition":2
    }
}
